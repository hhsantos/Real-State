# .env - Variables de entorno para Real State en producción
NODE_ENV=production
PORT=3002
HTTPS_PORT=443

# Configuración específica de Real State
APP_NAME=Real State
APP_VERSION=1.0.0
LOG_LEVEL=info

# Server info
SERVER_IP=18.184.20.26
SERVER_OS=Amazon Linux 2023
DOMAIN_NAME=https://realstate.dev.dreamsite.es

# URLs y dominios
DOMAIN=https://realstate.dev.dreamsite.es
CANONICAL_URL=https://realstate.dev.dreamsite.es

# Configuración de seguridad
CORS_ORIGIN=https://realstate.dev.dreamsite.es
TRUST_PROXY=true

# Configuración de performance
COMPRESSION_LEVEL=6
STATIC_CACHE_MAX_AGE=31536000

# Configuración de SSL (cuando esté disponible)
SSL_CERT_PATH=/var/www/ssl/fullchain.pem
SSL_KEY_PATH=/var/www/ssl/privkey.pem

